{
  "name": "server",
  "version": "1.0.0",
  "description": "INFO7255 Web API Server",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm run clean && npm run build:webapp && npm run build:graphql && npm run build:consumer",
    "build:server": "NODE_ENV=production webpack",
    "build:webapp": "tsc -p webapp/tsconfig.json",
    "build:graphql": "tsc -p graphql/tsconfig.json",
    "build:consumer": "tsc -p consumer/tsconfig.json",
    "postbuild": "cp package*.json dist && cd dist && npm ci --omit=dev",
    "dev:webpack": "npm dev:build && nodemon ./dist/webapp.js",
    "dev:build": "NODE_ENV=development webpack",
    "dev": "ts-node-transpile-only -p tsconfig.development.json webapp/server.ts",
    "dev:old": "tsc --watch webapp/server.ts",
    "dev:consumer": "ts-node -p tsconfig.development.json consumer/plan.consumer.ts",
    "dev:graphql": "ts-node -p tsconfig.development.json graphql/graphql-server.ts",
    "start": "concurrently \"npm run start:webapp\" \"npm run start:graphql\"",
    "start:webapp": "node webapp.js",
    "start:consumer": "node consumer.js",
    "start:graphql": "node graphql.js",
    "lint": "eslint ./**/*.ts",
    "lint:fix": "eslint ./**/*.ts --fix",
    "test": "mocha --exit",
    "test:watch": "mocha --watch",
    "coverage": "nyc --all --reporter=text npm run test",
    "redis-ui": "redis-commander --port 8081 --redis-host redis --redis-port 6379"
  },
  "author": {
    "firstname": "Venkata Naga Sai Teja",
    "lastname": "Marlapati",
    "NUID": "002756088",
    "name": "Venkata Naga Sai Teja Marlapati",
    "email": "marlapati.v@northeastern.edu"
  },
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^8.13.0",
    "ajv": "^6.12.6",
    "amqplib": "^0.10.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "etag": "^1.8.1",
    "express": "^4.19.2",
    "express-http-context": "1.2.4",
    "fast-json-patch": "^3.1.1",
    "google-auth-library": "^9.6.3",
    "graphql": "^16.8.1",
    "graphql-http": "^1.22.0",
    "http-status-codes": "^2.3.0",
    "json-merge-patch": "^1.0.2",
    "redis": "^4.6.13",
    "redis-commander": "git@github.com:marlapativ/redis-commander.git",
    "ruru": "^2.0.0-beta.11",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/chai": "^4.3.11",
    "@types/cors": "^2.8.17",
    "@types/etag": "^1.8.3",
    "@types/express": "^4.17.21",
    "@types/json-merge-patch": "^0.0.10",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.7",
    "@types/webpack": "^5.28.5",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "mocha": "^10.2.0",
    "nodemon": "^3.1.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.4",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "ts-node": "10.9.1",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
